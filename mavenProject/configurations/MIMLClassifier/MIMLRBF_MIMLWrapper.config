<configuration>
	<classifier name="miml.classifiers.miml.neural.MIMLRBF_MIMLWrapper">	
		<ratio>0.1</ratio>
		<mu>0.6</mu>
        <seed>1</seed>		
	</classifier>
	
	<evaluator name="miml.evaluation.EvaluatorHoldout">
		<data>
			<trainFile>data/miml_birds.arff</trainFile>
			<xmlFile>data/miml_birds.xml</xmlFile>
			<percentageTrain>80</percentageTrain>
		</data>
	</evaluator>
	
	<report name="miml.report.BaseMIMLReport">
		<fileName>results/MIMLClassifier/MIMLRBF_wrapper.csv</fileName>
	</report>
	
</configuration>